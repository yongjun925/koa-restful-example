{
  "extends": "standard",
  "rules": {
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "constructor-super": "warn",
    "valid-typeof": "warn",
    "semi": [1, "never"],
    "object-curly-spacing": [1, "always"],
    "comma-dangle": "off"
  }
}
